#AuthType Basic
#AuthName "Password Protected Area"
#AuthUserFile /var/www/html/restoreandretouch.me/.htpasswd
#Require valid-user

ErrorDocument 404 https://www.restoreandretouch.me/404.html
# BEGIN REDIRECT TO WWW ##
<IfModule mod_rewrite.c>
RewriteEngine On

RewriteCond %{HTTP_HOST} !^www\. [NC]
RewriteRule .* https://www.%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

</IfModule>
# END REDIRECT TO WWW ##

